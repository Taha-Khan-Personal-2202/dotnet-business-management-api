{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=dbNameHere;Username=username;Password=password"
  },

  "Jwt": {
    "Key": "JWT_SECRET_KEY",
    "Issuer": "DotNetBusinessWorkFlow",
    "Audience": "DotNetBusinessWorkFlowUsers",
    "ExpiryMinutes": 60
  },

  "Email": {
    "From": "YOUR_EMAIL_ID",
    "SmtpHost": "SMTP_HOST",
    "Port": 587,
    "Username": "USERNAME",
    "Password": "PASSKEY"
  },

  "Application": {
    "SeedData": true
  }
}